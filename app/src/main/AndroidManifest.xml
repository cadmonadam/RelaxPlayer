<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.relaxplayer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Birds">
            <intent-filter>
                <action android:name="android.intent.action.BIRDS" />
            </intent-filter>
        </activity>

        <activity android:name=".Download">
            <intent-filter>
                <action android:name="android.intent.action.DOWNLOAD" />
            </intent-filter>
        </activity>

        <activity android:name=".Mylibrary">
            <intent-filter>
                <action android:name="android.intent.action.MYLIBRARY" />
            </intent-filter>
        </activity>

        <activity android:name=".Nocturnal">
            <intent-filter>
                <action android:name="android.intent.action.NOCTURNAL" />
            </intent-filter>
        </activity>

        <activity android:name=".Nowplaying">
            <intent-filter>
                <action android:name="android.intent.action.NOWPLAYING" />
            </intent-filter>
        </activity>

        <activity android:name=".Storm">
            <intent-filter>
                <action android:name="android.intent.action.STORM" />
            </intent-filter>
        </activity>

    </application>

</manifest>